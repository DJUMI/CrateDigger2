{
  "expo": {
    "name": "CrateDigger",
    "slug": "CrateDigger",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b121c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "bundleIdentifier": "com.jiggyjamz.cratedigger",
      "supportsTablet": true
    },
    "android": {
      "package": "com.jiggyjamz.cratedigger",
      "versionCode": 3
    }
  }
}
